{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for trackmix.party",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "start": "tsc -p . && node index.js",
    "start-prod": "node index.js",
    "watch": "npm-watch start",
    "watch-prod": "npm-watch start-prod"
  },
  "author": "Nicholas Ricci",
  "license": "ISC",
  "watch": {
    "start": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "ts"
      ],
      "legacyWatch": true,
      "delay": 2500
    }
  },
  "dependencies": {
    "@types/busboy": "^1.5.3",
    "@types/spotify-web-api-node": "^5.0.11",
    "bcrypt": "^5.1.1",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "gridfs-stream": "^1.1.1",
    "mongoose": "^8.2.4",
    "multer": "^1.4.4",
    "multer-gridfs-storage": "^5.0.2",
    "npm-watch": "^0.11.0",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/mongoose": "^5.11.97",
    "typescript": "^5.4.3"
  }
}